[alias]
	co = checkout
	ci = commit -a -v
	st = status
	br = branch
	cl = clean -f -d
	ru = remote update
	rs = remote show origin
	recent = log --abbrev-commit --pretty=oneline -10
	last = log -p -1
[color]
	status = auto
	branch = auto
	ui = auto
[core]
	pager = less -FRSX
